{
  "name": "acperience",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "tomoya.otsuka@imjp.co.jp <>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build:prd": "DEPLOY_ENV=PRD nuxt build",
    "generate:prd": "DEPLOY_ENV=PRD nuxt generate && rm -rf docs/* && cp -r dist/ docs/"
  },
  "dependencies": {
    "@nuxtjs/google-tag-manager": "^2.0.0",
    "autoprefixer": "^9.1.1",
    "contentful": "^7.0.0",
    "node-sass": "^4.9.3",
    "nuxt": "^1.0.0",
    "sass-loader": "^7.1.0",
    "vue-markdown": "^2.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
